<%= simple_form_for(@restaurant) do |f|%>
  <%= f.input :name %>
  <%= f.input :address, :as => :text %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection: Restaurant::CATEGORIES %>

  <div class="form-actions btn btn-dark rounded">
    <%= f.button :submit, :class => 'text-white' %>
  </div>
<% end %>
